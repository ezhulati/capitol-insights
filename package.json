  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ci": "npm ci && vite build",
    "build:netlify": "chmod +x ./build.sh && ./build.sh",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "security-audit": "npm audit",
