{
  "name": "capitol-insights-website-netlify",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ci": "npm ci --legacy-peer-deps --force || npm install --legacy-peer-deps --force && vite build",
    "build:netlify": "chmod +x ./build.sh && ./build.sh",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "security-fix-force": "npm audit fix --force",
    "snyk-test": "snyk test",
    "snyk-monitor": "snyk monitor",
    "audit-resolve": "resolve-audit",
    "install-deps": "npm install --legacy-peer-deps",
    "install-deps:force": "npm install --force",
    "convert-images": "node convert-images-to-webp.js",
    "find-images": "node find-image-components.js",
    "analyze-js": "node optimize-js.js",
    "optimize": "npm run convert-images && npm run analyze-js",
    "optimize:all": "npm run convert-images && npm run find-images && npm run analyze-js",
    "optimize:bundle": "node optimize-bundle.js",
    "build:optimized": "npm ci --legacy-peer-deps --force || npm install --legacy-peer-deps --force && vite build && npm run optimize:bundle"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@mdx-js/loader": "2.0.0",
    "@mdx-js/react": "2.0.0",
    "@portabletext/react": "^3.2.1",
    "@sanity/client": "^6.28.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jspdf": "^2.0.0",
    "framer-motion": "^11.0.5",
    "gray-matter": "^4.0.3",
    "html-pdf": "^3.0.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "pdfkit": "^0.16.0",
    "puppeteer": "^24.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.4",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.22.3",
    "sharp": "^0.33.5",
    "validator": "^13.15.0",
    "web-vitals": "^3.5.2",
    "xss": "^1.0.15"
  },
  "overrides": {
    "@types/react-dom": "18.2.18",
    "@types/react": "18.2.48",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "resolutions": {
    "@types/react-dom": "18.2.18",
    "@types/react": "18.2.48",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.13.14",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chalk": "^5.4.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "imagesloaded": "^5.0.0",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "snyk": "^1.1296.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^4.5.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.4.0",
    "vite-plugin-pwa": "^0.21.2"
  }
}
